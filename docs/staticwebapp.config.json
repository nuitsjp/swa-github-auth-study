{
  "routes": [
    {
      "route": "/index.html",
      "allowedRoles": ["github_collaborator"]
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/js/*", "/static/css/*", "*.json"]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github",
      "statusCode": 302
    }
  }
}
